{
	"name": "PL_Race_results_Parent",
	"properties": {
		"activities": [
			{
				"name": "act_Web1",
				"description": "To get limit value",
				"type": "WebActivity",
				"dependsOn": [],
				"policy": {
					"timeout": "0.12:00:00",
					"retry": 1,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [],
				"typeProperties": {
					"method": "GET",
					"url": "http://ergast.com/api/f1/seasons.json"
				}
			},
			{
				"name": "act_Set_variable1",
				"description": "This will save limit value  in 'Limit' variable",
				"type": "SetVariable",
				"dependsOn": [
					{
						"activity": "act_Web1",
						"dependencyConditions": [
							"Succeeded"
						]
					}
				],
				"policy": {
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [],
				"typeProperties": {
					"variableName": "Limit",
					"value": {
						"value": "@activity('act_Web1').output.MRData.total",
						"type": "Expression"
					}
				}
			},
			{
				"name": "act_Web2",
				"description": "This activity will get data from API to ADLS",
				"type": "WebActivity",
				"dependsOn": [
					{
						"activity": "act_Set_variable1",
						"dependencyConditions": [
							"Succeeded"
						]
					}
				],
				"policy": {
					"timeout": "0.12:00:00",
					"retry": 1,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [],
				"typeProperties": {
					"method": "GET",
					"url": {
						"value": "@concat('http://ergast.com/api/f1/seasons.json?limit=',variables('Limit'))",
						"type": "Expression"
					}
				}
			},
			{
				"name": "act_ForEach_season",
				"description": "This activity will iterate season values",
				"type": "ForEach",
				"dependsOn": [
					{
						"activity": "act_Web2",
						"dependencyConditions": [
							"Succeeded"
						]
					}
				],
				"userProperties": [],
				"typeProperties": {
					"items": {
						"value": "@activity('act_Web2').output.MRData.SeasonTable.Seasons",
						"type": "Expression"
					},
					"isSequential": true,
					"activities": [
						{
							"name": "act_Set_variable_season_wise",
							"description": "This activity will write season value in one variable",
							"type": "SetVariable",
							"dependsOn": [
								{
									"activity": "act_Copy_data_Races",
									"dependencyConditions": [
										"Succeeded"
									]
								}
							],
							"policy": {
								"secureOutput": false,
								"secureInput": false
							},
							"userProperties": [],
							"typeProperties": {
								"variableName": "yearwise",
								"value": {
									"value": "@item().season",
									"type": "Expression"
								}
							}
						},
						{
							"name": "Execute Pipeline1",
							"type": "ExecutePipeline",
							"dependsOn": [
								{
									"activity": "act_Set_variable_season_wise",
									"dependencyConditions": [
										"Succeeded"
									]
								}
							],
							"policy": {
								"secureInput": false
							},
							"userProperties": [],
							"typeProperties": {
								"pipeline": {
									"referenceName": "PL_Race_Result_round_wise_child",
									"type": "PipelineReference"
								},
								"waitOnCompletion": true,
								"parameters": {
									"season_value_param": {
										"value": "@variables('yearwise')",
										"type": "Expression"
									}
								}
							}
						},
						{
							"name": "act_Web1_Races_data",
							"description": "This activity will get the data from API and store in ADLS Gen2 in JSON format",
							"type": "WebActivity",
							"dependsOn": [],
							"policy": {
								"timeout": "0.12:00:00",
								"retry": 2,
								"retryIntervalInSeconds": 30,
								"secureOutput": false,
								"secureInput": false
							},
							"userProperties": [],
							"typeProperties": {
								"method": "GET",
								"url": {
									"value": "@concat('http://ergast.com/api/f1/',item().season,'.json?limit=100')",
									"type": "Expression"
								}
							}
						},
						{
							"name": "act_Set_variable_races_data",
							"description": "This activity will write races data into one string type variable",
							"type": "SetVariable",
							"dependsOn": [
								{
									"activity": "act_Web1_Races_data",
									"dependencyConditions": [
										"Succeeded"
									]
								}
							],
							"policy": {
								"secureOutput": false,
								"secureInput": false
							},
							"userProperties": [],
							"typeProperties": {
								"variableName": "Races_data",
								"value": {
									"value": "@{activity('act_Web1_Races_data').output.MRData.RaceTable.Races}",
									"type": "Expression"
								}
							}
						},
						{
							"name": "act_Copy_data_Races",
							"description": "This activity will write data from set variable to adls bronze layer",
							"type": "Copy",
							"dependsOn": [
								{
									"activity": "act_Set_variable_races_data",
									"dependencyConditions": [
										"Succeeded"
									]
								}
							],
							"policy": {
								"timeout": "0.12:00:00",
								"retry": 1,
								"retryIntervalInSeconds": 30,
								"secureOutput": false,
								"secureInput": false
							},
							"userProperties": [],
							"typeProperties": {
								"source": {
									"type": "DelimitedTextSource",
									"additionalColumns": [
										{
											"name": "my_column",
											"value": {
												"value": "@variables('Races_data')",
												"type": "Expression"
											}
										}
									],
									"storeSettings": {
										"type": "AzureBlobFSReadSettings",
										"recursive": true,
										"enablePartitionDiscovery": false
									},
									"formatSettings": {
										"type": "DelimitedTextReadSettings"
									}
								},
								"sink": {
									"type": "DelimitedTextSink",
									"storeSettings": {
										"type": "AzureBlobFSWriteSettings"
									},
									"formatSettings": {
										"type": "DelimitedTextWriteSettings",
										"quoteAllText": true,
										"fileExtension": ".txt"
									}
								},
								"enableStaging": false,
								"translator": {
									"type": "TabularTranslator",
									"mappings": [
										{
											"source": {
												"name": "my_column",
												"type": "String"
											},
											"sink": {
												"type": "String",
												"physicalType": "String",
												"ordinal": 1
											}
										}
									],
									"typeConversion": true,
									"typeConversionSettings": {
										"allowDataTruncation": true,
										"treatBooleanAsNumber": false
									}
								}
							},
							"inputs": [
								{
									"referenceName": "DS_SOURCE_DATA",
									"type": "DatasetReference"
								}
							],
							"outputs": [
								{
									"referenceName": "DS_SINK_RACES_SEASON_WISE_DATA",
									"type": "DatasetReference"
								}
							]
						}
					]
				}
			},
			{
				"name": "act_Set_variable_only_season_data",
				"description": "This activity will write data from API to one string type variable",
				"type": "SetVariable",
				"dependsOn": [
					{
						"activity": "act_Web2",
						"dependencyConditions": [
							"Succeeded"
						]
					}
				],
				"policy": {
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [],
				"typeProperties": {
					"variableName": "season_data_only",
					"value": {
						"value": "@{activity('act_Web2').output.MRData.SeasonTable.Seasons}",
						"type": "Expression"
					}
				}
			},
			{
				"name": "act_Copy_data1",
				"description": "This activity will write season data from set variable to adls in JSON format",
				"type": "Copy",
				"dependsOn": [
					{
						"activity": "act_Set_variable_only_season_data",
						"dependencyConditions": [
							"Succeeded"
						]
					}
				],
				"policy": {
					"timeout": "0.12:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [],
				"typeProperties": {
					"source": {
						"type": "DelimitedTextSource",
						"additionalColumns": [
							{
								"name": "mycolumns",
								"value": {
									"value": "@variables('season_data_only')",
									"type": "Expression"
								}
							}
						],
						"storeSettings": {
							"type": "AzureBlobFSReadSettings",
							"recursive": true,
							"enablePartitionDiscovery": false
						},
						"formatSettings": {
							"type": "DelimitedTextReadSettings"
						}
					},
					"sink": {
						"type": "DelimitedTextSink",
						"storeSettings": {
							"type": "AzureBlobFSWriteSettings"
						},
						"formatSettings": {
							"type": "DelimitedTextWriteSettings",
							"quoteAllText": true,
							"fileExtension": ".txt"
						}
					},
					"enableStaging": false,
					"translator": {
						"type": "TabularTranslator",
						"mappings": [
							{
								"source": {
									"name": "mycolumns",
									"type": "String"
								},
								"sink": {
									"type": "String",
									"physicalType": "String",
									"ordinal": 1
								}
							}
						],
						"typeConversion": true,
						"typeConversionSettings": {
							"allowDataTruncation": true,
							"treatBooleanAsNumber": false
						}
					}
				},
				"inputs": [
					{
						"referenceName": "DS_SOURCE_DATA",
						"type": "DatasetReference"
					}
				],
				"outputs": [
					{
						"referenceName": "DS_SINK_SEASONS_DATA_ONLY",
						"type": "DatasetReference"
					}
				]
			}
		],
		"variables": {
			"Limit": {
				"type": "String"
			},
			"Seasons": {
				"type": "String"
			},
			"total_seasons": {
				"type": "Array"
			},
			"Final_Total_Seasons": {
				"type": "String"
			},
			"SEASON": {
				"type": "String"
			},
			"race_result": {
				"type": "String"
			},
			"filename_season": {
				"type": "String"
			},
			"yearwise": {
				"type": "String"
			},
			"season_data": {
				"type": "String"
			},
			"season_data_only": {
				"type": "String"
			},
			"Races_data": {
				"type": "String"
			}
		},
		"annotations": [],
		"lastPublishTime": "2024-04-11T11:31:42Z"
	},
	"type": "Microsoft.DataFactory/factories/pipelines"
}