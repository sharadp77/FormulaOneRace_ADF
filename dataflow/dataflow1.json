{
	"name": "dataflow1",
	"properties": {
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"dataset": {
						"referenceName": "DS_HR_Employees",
						"type": "DatasetReference"
					},
					"name": "HREmployees"
				},
				{
					"dataset": {
						"referenceName": "DS_HR_DEPARTMENTS",
						"type": "DatasetReference"
					},
					"name": "HrDepartments"
				}
			],
			"sinks": [
				{
					"dataset": {
						"referenceName": "DS_SINK_OUTPUT_DATA",
						"type": "DatasetReference"
					},
					"name": "sinkOutputData"
				}
			],
			"transformations": [
				{
					"name": "joinEmployeesOnDepartments"
				},
				{
					"name": "filter1"
				}
			],
			"scriptLines": [
				"source(output(",
				"          EMPLOYEE_ID as string,",
				"          FIRST_NAME as string,",
				"          LAST_NAME as string,",
				"          EMAIL as string,",
				"          PHONE_NUMBER as string,",
				"          HIRE_DATE as string,",
				"          JOB_ID as string,",
				"          SALARY as string,",
				"          COMMISSION_PCT as string,",
				"          MANAGER_ID as string,",
				"          DEPARTMENT_ID as string",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     ignoreNoFilesFound: false) ~> HREmployees",
				"source(output(",
				"          DEPARTMENT_ID as string,",
				"          DEPARTMENT_NAME as string,",
				"          MANAGER_ID as string,",
				"          LOCATION_ID as string",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     ignoreNoFilesFound: false) ~> HrDepartments",
				"HREmployees, HrDepartments join(HREmployees@DEPARTMENT_ID == HrDepartments@DEPARTMENT_ID,",
				"     joinType:'inner',",
				"     matchType:'exact',",
				"     ignoreSpaces: false,",
				"     broadcast: 'auto')~> joinEmployeesOnDepartments",
				"joinEmployeesOnDepartments filter(equals((HrDepartments@DEPARTMENT_ID), '30')) ~> filter1",
				"filter1 sink(allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     filePattern:'join.csv',",
				"     umask: 0022,",
				"     preCommands: [],",
				"     postCommands: [],",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true,",
				"     mapColumn(",
				"          EMPLOYEE_ID,",
				"          FIRST_NAME,",
				"          LAST_NAME,",
				"          EMAIL,",
				"          PHONE_NUMBER,",
				"          HIRE_DATE,",
				"          JOB_ID,",
				"          SALARY,",
				"          COMMISSION_PCT,",
				"          MANAGER_ID = HREmployees@MANAGER_ID,",
				"          DEPARTMENT_ID = HREmployees@DEPARTMENT_ID,",
				"          DEPARTMENT_NAME,",
				"          MANAGER_ID = HrDepartments@MANAGER_ID,",
				"          LOCATION_ID",
				"     )) ~> sinkOutputData"
			]
		}
	}
}